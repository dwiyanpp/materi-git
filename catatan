// akun
git config --global user.name "username"
git config --global user.email "email"
git config --list

// status
git add .
git commit -m "pesan"
git status

// log
git log
gil log -2
gil log --all --decorate --oneline --graph

// branch
git branch
git branch namaBranch

// merge
git branch --merged
git merge master

// delete
git branch -d namaBranch

// checkout
git checkout 5digitHash
git checkout namaBranch

// clone
git clone linkOrSSL

// create folder
mkdir newFolder

// position
pwd
ls

// remote
git remote
git push
git fetch
git pull
